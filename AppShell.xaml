<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="LensEyed.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:LensEyed.Views"
	xmlns:local="clr-namespace:LensEyed">

	<TabBar>
		<ShellContent
			Title="Main"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:MainPage}" />
		<ShellContent
			Title="ListDetail"
			Icon="iconlistdetail.png"
			ContentTemplate="{DataTemplate views:ListDetailPage}" />
		<ShellContent
			Title="Drawing"
			Icon="icondrawing.png"
			ContentTemplate="{DataTemplate views:DrawingPage}" />
	</TabBar>
</Shell>
